<?xml version="1.0"?>

<onPacket
        xmlns="https://www.snlab.org"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://www.snlab.org ../visualprog.xsd">
    <branchs>
        <branch>
            <code>
                a = 10;
                b = 20;
            </code>
            <condition>a &lt; 5;</condition>
            <compounds>
                <compound>
                    <logic>and</logic>
                    <branch>
                        <condition>a &lt; b</condition>
                        <behavior id="1">
                            <algorithm>
                                <customizedAlgorithm>
                                    <name>sum</name>
                                    <code>return a + b</code>
                                </customizedAlgorithm>
                            </algorithm>
                        </behavior>
                    </branch>
                </compound>
                <compound>
                    <logic>or</logic>
                    <branch>
                        <condition>b &gt; 25</condition>
                        <behavior id="2">
                            <algorithm>
                                <simpleAlgorithm>
                                    <name>subtraction</name>
                                    <args>
                                        <arg>10</arg>
                                        <arg>20</arg>
                                    </args>
                                </simpleAlgorithm>
                            </algorithm>
                        </behavior>
                    </branch>
                </compound>
            </compounds>
        </branch>
        <branch>
            <code>c=20;d=50;</code>
            <condition>c&lt;d</condition>
            <behavior id="3">
                <algorithm>
                    <simpleAlgorithm>
                        <name>power</name>
                        <args>
                            <arg>c</arg>
                            <arg>d</arg>
                        </args>
                    </simpleAlgorithm>
                </algorithm>
            </behavior>
        </branch>
    </branchs>
</onPacket>